{
  "vm_box": "ubuntu/jammy64",
  "vm_box_version": "20241002.0.0",
  "box_check_update": true,
  "vm_hostname": "dev-env",
  "vm_memory": "6144",
  "vm_cpus": 2,
  "vb_gui": false,
  "graphics_controller": "vmsvga",
  "vb_clipboard": "bidirectional",
  "ssh_forward_x11": true,
  "ssh_insert_key": false,
  "forward_jupyter_port": true,
  "port_forwarding": [
    { "guest": 8888, "host": 8888 }
  ],
  "synced_folders": [
    {
      "guest": "/home/vagrant/TruWork",
      "host": "~/TruWork",
      "create": true
    }
  ],
  "provision": [
    {
      "type": "shell",
      "path": "vagrant-scripts/vagrant_setup_arch.sh"
    }
  ],
  "networks": [
    {
      "type": "private_network",
      "dhcp": true
    }
  ],
  "environment_variables": {},
  "post_up_message": "Port forwarding for Jupyter is enabled on port 8888.",
  "user_shell": "zsh",
  "open_url": "http://www.google.com"
}
