{
  "vm_box": "ubuntu/jammy64",
  "vm_box_version": "20240823.0.1",
  "box_check_update": true,
  "vm_hostname": "dev-env",
  "vm_memory": "4096",
  "vm_cpus": 2,
  "vb_gui": false,
  "ssh_forward_x11": true,
  "ssh_insert_key": false,
  "forward_jupyter_port": true,
  "port_forwarding": [
    { "guest": 8888, "host": 8888 }
  ],
  "synced_folders": [],
  "provision": [
    {
      "type": "shell",
      "path": "vagrant_setup.sh"
    }
  ],
  "networks": [
    {
      "type": "private_network",
      "dhcp": true
    }
  ],
  "environment_variables": {},
  "post_up_message": "Port forwarding for Jupyter is enabled on port 8888.",
  "user_shell": "bash"  // Change to "zsh" if using zsh
}